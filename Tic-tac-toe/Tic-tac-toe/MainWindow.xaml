        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Height="55" Margin="240,206,0,0" VerticalAlignment="Top" Width="64" Click="button_Click"/>
        <Button x:Name="button_Copy" Content="Button" HorizontalAlignment="Left" Height="55" Margin="240,123,0,0" VerticalAlignment="Top" Width="64" Click="button_Copy_Click"/>
        <Button x:Name="button_Copy1" Content="Button" HorizontalAlignment="Left" Height="55" Margin="155,41,0,0" VerticalAlignment="Top" Width="64" Click="button_Copy1_Click"/>
        <Button x:Name="button_Copy2" Content="Button" HorizontalAlignment="Left" Height="55" Margin="240,41,0,0" VerticalAlignment="Top" Width="64" Click="button_Copy2_Click"/>
