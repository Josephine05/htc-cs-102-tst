        <TextBlock x:Name="Xsbox" HorizontalAlignment="Left" Height="45" Margin="224,294,0,0" TextWrapping="Wrap" Text="X" VerticalAlignment="Top" Width="29" RenderTransformOrigin="0.626,-0.549" FontSize="35" FontFamily="Papyrus"/>
